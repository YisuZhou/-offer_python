# -*- coding:utf-8 -*-
class Solution:
    def Add(self, num1, num2):
        # write code here
        #整数，注意考虑正负
        #能及时想到异或^、与&、移位<<操作
        #但是具体细节脑补缺失
        while num2:
            temp_nor = num1 ^ num2
            temp_and = num1 & num2
            num2 = temp_and << 1
            num1 = temp_nor & 0xFFFFFFFF  #这步想不到
        if num1 & 0x80000000:
            return num1-2**32
        else:
            return num1
