# -*- coding:utf-8 -*-
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None
class Solution:
    def FindFirstCommonNode(self, pHead1, pHead2):
        # write code here
        # 用最费时最简单的方案尝试
        p2 = pHead2
        while pHead1 :
            while pHead2:
                if pHead2 == pHead1:
                    return pHead2
                else:
                    pHead2 = pHead2.next
            pHead2 = p2
            pHead1 = pHead1.next   
        return None
